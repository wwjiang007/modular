executable("bootstrap") {
  sources = [
    "app.cc",
    "app.h",
    "config.cc",
    "config.h",
    "delegating_application_loader.cc",
    "delegating_application_loader.h",
    "main.cc",
    "params.cc",
    "params.h",
  ]

  deps = [
    "//application/lib/app",
    "//apps/modular/lib/rapidjson",
    "//lib/fidl/cpp/bindings",
    "//lib/ftl",
    "//lib/mtl",
    "//lib/url",
  ]
}
